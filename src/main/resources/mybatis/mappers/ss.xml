<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ss">
	<!-- 쿼리문작성 -->
	<!-- <![CDATA[]]> -->
	<select id="checkId" parameterType="String" resultType="com.javaex.vo.UserJoinVo">
		<![CDATA[
			select	user_id
			from user
			where user_id = #{id}
		]]>
	</select>

</mapper>